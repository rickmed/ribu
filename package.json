{
  "name": "ribu",
  "description": "A Concurrency Library for Javascript",
  "version": "0.1.2",
  "files": [
    "source",
    "test-utils"
  ],
  "exports": {
    ".": {
      "import": "./source/index.mjs"
    },
    "./test": {
      "import": "./tests/test-utils.mjs"
    }
  },
  "type": "module",
  "scripts": {
    "test": "clear; vitest --reporter=basic --watch=false",
    "test:watch": "clear; vitest -w --reporter=basic",
    "test:cov": "cross-env 'NODE_OPTIONS=--import @istanbuljs/esm-loader-hook --no-warnings' nyc npm test",
    "type-check": "tsc --noEmit",
    "build": "rm -rf dist; tsc"
  },
  "engines": {},
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-functional": "^6.5.1",
    "tsx": "^3.12.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rickmed/ribu"
  },
  "issues": "https://github.com/rickmed/ribu/issues",
  "license": "MIT",
  "author": {
    "name": "Ricardo Medina"
  },
  "keywords": [
    "concurrency",
    "fast",
    "js",
    "javascript",
    "typescript"
  ]
}
